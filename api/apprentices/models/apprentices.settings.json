{
  "kind": "collectionType",
  "collectionName": "apprentices",
  "info": {
    "name": "Apprentices",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
